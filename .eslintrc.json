{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
     },
    "extends": "airbnb-base",   
    "parserOptions":{    
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ]
    }
}